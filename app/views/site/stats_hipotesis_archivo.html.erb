<% mftext('Experimentos completados') do %>
<div>
<% @ab_tests = AbTest.paginate(:conditions => "completed_on IS NOT NULL AND active = 't'", :order => 'completed_on DESC', :page => params[:page], :per_page => 50) %>
<%= render :partial => 'shared/pager2', :locals => {:collection => @ab_tests, 'pos' => 'top'} %>
<table>
<% @ab_tests.each do |t| %>
<%= render :partial => 'stats_hipotesis_tr', :locals => { :t => t} %>
<% end -%>
</table>
<br />
<%= render :partial => 'shared/pager2', :locals => {:collection => @ab_tests, 'pos' => 'bottom'} %>
<strong><a href="/site/stats_hipotesis">Volver a experimentos activos</a></strong>
</div>
<% end -%>
